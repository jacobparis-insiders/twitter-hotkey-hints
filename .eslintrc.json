{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": "error",
    "one-var": ["error", "never"],
    "func-style": ["error", "declaration", { "allowArrowFunctions": true }],
    "prefer-arrow-callback": "error",
    "no-param-reassign": "error"
  }
}
